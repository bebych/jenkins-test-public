pipeline {
  agent any
  stages {
    stage('Buzz Buzz') {
      steps {
        echo 'HELLO'
      }
    }

    stage('Bees Bees') {
      steps {
        echo 'world'
      }
    }

    stage('sleep') {
      steps {
        sh 'sleep 5'
      }
    }

    stage('success') {
      steps {
        echo 'success'
      }
    }

  }
}